{
  "name": "sdk-example",
  "description": "An example website that uses Schibsted's new browser SDK",
  "version": "5.9.3",
  "license": "MIT",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.schibsted.io/spt-identity/sdk-example.com"
  },
  "scripts": {
    "build": "./build.sh",
    "dev": "export NODE_OPTIONS=--openssl-legacy-provider && ./watch.sh && nodemon -w . -w browser -w dist server.js",
    "lint": "eslint ."
  },
  "dependencies": {
    "@schibsted/account-sdk-browser": "^4.8.4",
    "cookie-parser": "^1.4.4",
    "dotenv": "^5.0.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "getenv": "^0.7.0",
    "hbs": "^4.1.1",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.0",
    "passport": "^0.4.0",
    "passport-oauth2": "^1.5.0",
    "passport-oauth2-middleware": "^1.0.3",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "token-introspection": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.23.2",
    "babel-loader": "^8.1.0",
    "eslint": "^4.12.0",
    "eslint-plugin-import": "^2.18.0",
    "nodemon": "^2.0.4",
    "regenerator-runtime": "^0.11.1",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
