    "lint": "eslint ."
  },
  "dependencies": {
    "@schibsted/account-sdk-browser": "^4.8.6-beta.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.17.3",
    "express-session": "^1.17.3",
    "getenv": "^1.0.0",
    "hbs": "^4.2.0",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^9.0.0",
    "node-fetch": "^2.7.0",
    "passport": "^0.7.0",
    "passport-oauth2": "^1.6.1",
    "passport-oauth2-middleware": "^1.0.3",
    "axios": "^1.6.5",
    "token-introspection": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "babel-loader": "^9.1.3",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "nodemon": "^3.0.3",
    "regenerator-runtime": "^0.14.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
